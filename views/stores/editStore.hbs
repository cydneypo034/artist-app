<form method="POST" action="/artstore/{{store}}?_method=PUT">
    <label for="name">Name</label>
    <input type="text" name="name" value="{{name}}"/>

    <label for="address">Address</label>
    <input type="text" name="address" value="{{address}}" />

    <label for="yearsOpen">Years Open</label>
    <input type="text" name="yearsOpen" value="{{yearsOpen}}"/>

    <label for="website">Website</label>
    <input type="text" name="website" value="{{website}}"/>

    <label for="manager">Managers</label>
    <input type="text" name="managers" value="{{managers}}"/>

    <label for="availableClasses">Available Classes</label>
    <input type="text" name="availableClasses" value="{{availableClasses}}"/>

    <input type="submit" value="Save Store" />

</form>




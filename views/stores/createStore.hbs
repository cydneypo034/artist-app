
<form method="POST" action="/artstore/addstore">
    <div id="form-text">
    <div class="form-group">
    <label for="name">Name</label>
    <input type="text" name="name" placeholder="Name"/>
    </div>
    <div class="form-group">
    <label for="address">Address</label>
    <input type="text" name="address" placeholder="Address"/>
    </div>
    <div class="form-group">
    <label for="yearsOpen">Years Open</label>
    <input type="text" name="yearsOpen" placeholder="Enter A Number" />
    </div>
    <div class="form-group">
    <label for="website">Website</label>
    <input type="text" name="website" placeholder="Ex: www.storesite.com" />
    </div>
    <div class="form-group">
    <label for="availableClasses">Available Classes</label>
    <input type="text" name="availableClasses" placeholder="Ex: Painting"/>
    </div>
    </div>
    <input type="submit" class= "btn btn-default btn-success" value="Add New Store"/>
</form>

<br>

<a href="/artstore" class= "btn btn-default btn-info">View All Stores</a>

<a href="/materialstore" class= "btn btn-default btn-warning">View All Materials</a>



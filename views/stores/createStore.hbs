
<form method="POST" action="/artstore/addstore">
    <label for="name">Name</label>
    <input type="text" name="name" />

    <label for="address">Address</label>
    <input type="text" name="address"/>

    <label for="yearsOpen">Years Open</label>
    <input type="text" name="yearsOpen"/>

    <label for="website">Website</label>
    <input type="text" name="website"/>

    <label for="availableClasses">Available Classes</label>
    <input type="text" name="availableClasses"/>

    <input type="submit" class= "btn btn-default btn-success" value="Add New Store"/>
</form>

<a href="/artstore">View All Stores</a>

<a href="/materialstore">View All Materials</a>


